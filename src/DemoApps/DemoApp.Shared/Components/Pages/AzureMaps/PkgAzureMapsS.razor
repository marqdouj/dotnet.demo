<PageTitle>Azure Maps</PageTitle>

<FluentLayout>
	<FluentHeader>@header</FluentHeader>

	<FluentTabs Height="90%">
		<FluentTab Label="Azure Maps">
			<ReadMeView Repository="HRefRepository.AzureMaps" Height="@height" />
		</FluentTab>

		<FluentTab Label="Azure Maps UI">
			<ReadMeView Repository="HRefRepository.AzureMapsUI" Height="@height" />
		</FluentTab>
	</FluentTabs>
	
</FluentLayout>

@code {
	private string header => $"{HRefRepository.AzureMaps.Name()} - {HRefRepository.AzureMapsUI.Name()}";
	private string height = "600px";
}
